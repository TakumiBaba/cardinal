div#like_page
  height: 100%
  width: 550px
  padding-left: 10px
  h3
    font-size: 16px
  small
    font-weight: normal
  h4.title_box
    padding: 5px
  div.likebox
    float: left
    height: 100%
    min-height: 170px
    width: 550px
    ul
      display: block
      float: left
      width: 100%
      height: 100%
      padding-left 5px
      li
        margin-right: 10px
        float: left
        width: 100px
        height: 170px
        div.thumbnail
          a
            text-align: center
            display: block
            width: 100%
          a.name
            color: black
          a.to-user
            display: block
            width: 100px
            text-align: center
            img
              height: 70px
              width: 70px
          a.action_button
            margin-bottom: 5px
            img
              width: 100%
          a.to-talk
            font-size: 12px
