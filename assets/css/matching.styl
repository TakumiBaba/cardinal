div#matching_page
  height: 100%
  width: 550px
  div.header
    width: 550px
    float: left
    ul#matching_type_list
      margin-top: 20px
      li
        a
          color: #555555
      li.active
        a
          color: #08c
          font-weight: bold

  div.body
    float: left
    width: 550px
    margin-left: 10px
    div.side
      float: left
      width: 70px
      max-height: 700px
      div.user_list_box
        overflow: hidden
        height: 370px
        margin-top: 5px
        margin-bottom: 5px
      ul.user_list
        margin-top: 0px
        padding-left: 0px
        li
          margin-bottom: 5px
          img
            width: 45px
            height: 45px
            border: 4px solid white
        li.active
          img
            border: 4px solid #ed4b7c
    div.profile
      padding-left: 10px
      width: 460px
      float: left
      div.profile_header
        width: 460px
        div.p_h_left
          float: left
          width: 100px
          img
            margin: 5px
            width: 90px
            height: 90px
        div.p_h_right
          float: left
          width: 355px
          h4.title_box
            float: left
            width: 355px
            margin-bottom: 5px
          small.message
            float: left
            width: 355px
            min-height: 70px
            font-size: 12px
          div.btns
            float: left
            width: 355px
            img
              margin-top: 10px
              margin-bottom: 10px
            img.like
              width: 80px
            img.message
              width: 120px
              margin-left: 5px
              margin-right: 5px
            img.talk
              width: 120px
      div.profile_supporter_messages
        width: 460px
        float: left
        ul
          li
            clear: left
            width: 450px
            div.s-message-left
              height: 100%
              width: 100px
              float: left
              img
                width: 90px
                height: 90px
            div.s-message-right
              height: 100%
              width: 350px
              float: left
              div.s-messge-header
                height: 20%
                width: 350px
                float: left
              div.s-message-body
                height: 79%
                width: 350px
                float: left
      div.profile_body
        float: left
        width: 460px
        table
          margin-bottom: 10px
          font-size: 12px
          td
            padding: 5px
        table.sub
          margin-top: 5px
          width: 460px
          tr
            height: 5px
          td.key
            background-color: #cef6ec
            width: 130px
          td.value
            width: 340px
        table.main
          width: 460px
          tr
            height: 5px
          td.left-key
            background-color: #cef6ec
            width: 100px
          td.left-value
            width: 100px
          td.right-key
            background-color: #cef6ec
            width: 130px
          td.right-value
            width: 140px
      div.profile_supporters
        width: 460px
        float: left
        li
          div.thumbnail
            width: 90px
            a.to-user
              text-align: center
          img
            width: 90px