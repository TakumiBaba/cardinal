  div#matchinglist
    height: 100%
    width: 549px
    h4
      float: left
      width: 550px
    div.likebox
      float: left
      ul
        margin: 0px
        display: block
        float: left
        width: 550px
        height: 100%
        padding-left 5px
        li
          margin-right: 10px
          float: left
          width: 100px
          height: 150px
          div.thumbnail
            a.to-user
              display: block
              width: 100px
              text-align: center
              h5
                margin: 5px
              img
                height: 70px
                width: 70px
            a.like
              img
                height: 30px
                width: 90px
            a.to-talk
