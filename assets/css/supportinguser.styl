  div#likelist
    div.likebox
      width: 100%
      margin-bottom: 30px
      height: 150px
      ul
        margin: 0px
        padding-left: 0px
        list-style: none
        li
          width: 150px
          height: 150px
          float: left
          margin-top: 20px
          div.thumbnail
            margin-right: 10px
            a
              margin: 5px
              display: block
              text-align: center
              img
                margin: 0 auto
                width: 70px
                height: 70px
            a.to-user
              h5
                margin: 5px


