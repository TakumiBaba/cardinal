div#user_page
  margin-left: 10px
  h3
    font-size: 16px
  small
    font-weight: normal
  h4.title_box
    padding: 5px
  div.profile_header
    width: 549px
    div.p_h_left
      float: left
      width: 100px
      img
        margin: 5px
        width: 90px
        height: 90px
    div.p_h_right
      float: left
      width: 440px
      h4.title_box
        float: left
        width: 440px
        margin-bottom: 5px
      small.message
        float: left
        width: 440px
        min-height: 70px
  div.detail_profile
    div.profile_column
      width: 549px
    div.profile_column_body
      h4.title_box
        width: 549px
      float: left
      width: 549px
      table
        margin-bottom: 10px
        font-size: 12px
        td
          padding: 5px
      table.sub
        width: 549px
        margin-top: 5px
        tbody
          tr
            height: 5px
            td.key
              background-color: #cef6ec
              width: 130px
            td.value
              width: 340px
      table.main
        width: 549px
        tr
          height: 5px
        td.left-key
          background-color: #cef6ec
          width: 100px
        td.left-value
          width: 100px
        td.right-key
          background-color: #cef6ec
          width: 130px
          font-weight: bold
        td.right-value
          width: 140px
    div.supporter-message-list
      width: 549px
      ul
        padding-left: 0px
        li
          margin-left: 0px
          padding-top: 15px
          margin-bottom: 15px
          div.s-message-left
            height: 100%
            width: 50px
            float: left
            img
              width: 40px
              height: 40px
              float: right
          div.s-message-right
            height: 100%
            width: 450px
            float: left
            margin-left: 5px
            div.s-messge-header
              height: 20%
              width: 450px
              float: left
            div.s-message-body
              height: 79%
              width: 450px
              float: left
