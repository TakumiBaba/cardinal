div#matching_page
  div.header
    ul#matching_type_list.nav.nav-tabs
      li.system.active: a お相手リスト
      li.supporter: a 応援団おすすめリスト
  div#system_matching.body
    div.side
      ul.user_list.system
      ul.user_list.supporter
    div.profile
      div.profile_header
        div.p_h_left
          img
        div.p_h_right
          h4.title_box
          p.message
          div.btns
            a(href="/#/like")
              img.like(src="/image/iine_button.gif")
            a(href="/#/message")
              img.message(src="/image/m_button.gif")
            a(href="/#/talk")
              img.talk(src="/image/o_button.gif")
      div.profile_supporter_messages
        h4.title_box
        ul
      div.profile_body
        h4.title_box
      div.profile_supporters
        h4.title_box
